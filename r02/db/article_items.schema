# frozen_string_literal: true

create_table :article_items, id: :bigint, unsigned: true, force: :cascade, options: 'ENGINE=InnoDB DEFAULT CHARSET=utf8mb4', comment: '議事録' do |t|
  t.integer :article_id, null: false

  t.string :text, null: false

  t.datetime :updated_at, null: false
  t.datetime :created_at, null: false

  t.index :article_id
end
